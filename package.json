{
  "name": "webpack-seed",
  "version": "1.0.0",
  "description": "A well-done webpack config to boost your web project",
  "repository": {
    "type": "git",
    "url": "https://github.com/ben-yip/webpack-seed.git"
  },
  "scripts": {
    "build": "webpack --config webpack.prod.js",
    "build-dev": "webpack --config webpack.dev.js",
    "start": "webpack-dev-server --config webpack.dev.js",
    "watch": "webpack --watch",
    "bs": "browser-sync start --server --port 8000 --ui-port 8001 --no-open --no-notify --files \"dist/*.*\""
  },
  "author": "BenYip",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.3",
    "babel-preset-env": "^1.6.1",
    "clean-webpack-plugin": "^0.1.18",
    "css-loader": "^0.28.10",
    "csv-loader": "^2.1.1",
    "extract-loader": "^1.0.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.10",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^2.30.1",
    "image-webpack-loader": "^4.2.0",
    "node-sass": "^4.8.3",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.19.1",
    "url-loader": "^1.0.1",
    "webpack": "^3.11.0",
    "webpack-dev-server": "^2.11.2",
    "webpack-merge": "^4.1.2",
    "webpack-visualizer-plugin": "^0.1.11",
    "xml-loader": "^1.2.1"
